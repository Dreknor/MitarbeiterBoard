{
    "name": "dreknor/mitarbeiterboard",
    "type": "project",
    "description": "MitarbeiterBoard des ESZ Radebeul",
    "keywords": [
        "school",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aacotroneo/laravel-saml2": "^2.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "composer/composer": "^2.2",
        "doctrine/dbal": "^3.5",
        "dyrynda/laravel-cascade-soft-deletes": "^4.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "laravel-notification-channels/webpush": "^7.0",
        "laravel/framework": "^10.0",
        "laravel/scout": "^10.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^4.0",
        "maatwebsite/excel": "^3.1",
        "owen-it/laravel-auditing": "^13.6",
        "phpoffice/phpword": "^1.0",
        "ryangjchandler/blade-cache-directive": "^0.3.1",
        "sentry/sentry-laravel": "^3.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "socialiteproviders/keycloak": "*",
        "socialiteproviders/zoho": "^4.1",
        "spatie/laravel-google-calendar": "^3.5",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-permission": "^5.5",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.16"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.2",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
